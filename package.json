{
  "name": "split",
  "version": "0.0.0",
  "description": "The Best way to start a sveltekit project.",
  "maintainers": [
    "Aymen keskas <keskasaymen8@gmail.com> (https://empty-void.vercel.app)"
  ],
  "engines": {
    "node": ">=18.x"
  },
  "settings": {
    "autoInstallPeers": false
  },
  "files": [
    "dist/*",
    "template/*",
    "template/base/.gitignore",
    "template/base/.npmrc"
  ],
  "exports": "./dist/index.js",
  "main": "dist/index.js",
  "bin": {
    "split": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "dev": "pnpm run build && pnpm run start",
    "start": "node dist/index.js",
    "build": "tsup",
    "lint": "tsc",
    "release": "tsup && changeset publish"
  },
  "keywords": [
    "split",
    "init.tips",
    "sveltekit",
    "split-stack",
    "tailwind",
    "zod",
    "typescript",
    "prisma"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EMPTYVOID-DEV/Split"
  },
  "author": "Keskas aymen",
  "license": "MIT",
  "dependencies": {
    "cfonts": "^3.2.0",
    "chalk": "5.2.0",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.8.6",
    "tsup": "^7.2.0",
    "type-fest": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
